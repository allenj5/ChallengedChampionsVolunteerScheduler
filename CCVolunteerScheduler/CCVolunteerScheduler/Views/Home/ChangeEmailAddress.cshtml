@{
    ViewBag.Title = "ChangeEmailAddress";
}

<div class="changeemail">
    <h2>Change Email Address</h2>
    <form class="changeemail_form">
        <p>New Email Address</p>
        <input type="email" name="" placeholder="Enter new email address">
        <p>Confirm New Email Address:</p>
        <input type="email" name="" placeholder="Confirm new email address">
        <p>Password:</p>
        <input type="password" name="" placeholder="Enter your password">
        <input type="submit" value="Submit" />
    </form>
</div>
